#!/usr/bin/env ruby
# frozen_string_literal: true

ENV['RACK_ENV'] ||= 'development'

require 'bundler/setup'
require 'oj_serializers'
require 'pry-byebug'

$:.unshift Pathname.new(__dir__).join('../spec')

Dir[Pathname.new(__dir__).join('../spec/support/**/*.rb')].each { |f| require f }

Pry.start
